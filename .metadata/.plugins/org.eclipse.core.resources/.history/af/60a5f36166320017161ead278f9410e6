import java.net.InetAddress;

public class Player {
int x,y;
InetAddress address;
public Player(int x, int y, InetAddress address) {
	super();
	this.x = x;
	this.y = y;
	this.address = address;
}
public int getX() {
	return x;
}
public void setX(int x) {
	this.x = x;
}
public int getY() {
	return y;
}
public void setY(int y) {
	this.y = y;
}
public InetAddress getAddress() {
	return address;
}
public void setAddress(InetAddress address) {
	this.address = address;
}
}
